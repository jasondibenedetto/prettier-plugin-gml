{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "camelcase": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "no-console": 0
  },
  "overrides": [
    {
      "files": "tests/**/*.js",
      "globals": {
        "run_spec": true
      }
    }
  ]
}
